{
  "name": "hancom-web-frontend",
  "version": "0.3.0",
  "private": true,
  "scripts": {
    "build-test": "NODE_ENV=test vue-cli-service build --mode test",
    "build-development": "set NODE_ENV=development&&vue-cli-service build --mode development",
    "build-stage": "NODE_ENV=stage vue-cli-service build --mode stage",
    "build-production": "NODE_ENV=production vue-cli-service build --mode production",
    "serve": "vue-cli-service serve --port 7302",
    "build": "vue-cli-service build",
    "test": "NODE_ENV=test npm run build",
    "dev": "NODE_ENV=development vue-cli-service build --mode development",
    "dev:watch": "NODE_ENV=development vue-cli-service build --mode development --watch",
    "stage": "NODE_ENV=stage vue-cli-service build --mode stage",
    "production": "NODE_ENV=production vue-cli-service build --mode production",
    "start": "set NODE_ENV=development&&nodemon ./bin/www"
  },
  "dependencies": {
    "@babel/core": "7.3.4",
    "@vue/b4abel-preset-app": "3.5.1",
    "autoprefixer": "9.4.9",
    "axios": "0.18.0",
    "debug": "2.6.9",
    "echarts": "4.2.1",
    "export-to-csv": "^0.2.1",
    "expose-loader": "0.7.5",
    "express": "4.16.0",
    "http-errors": "1.6.2",
    "jquery": "3.3.1",
    "jquery-ui": "1.12.1",
    "moment": "2.24.0",
    "morgan": "1.9.0",
    "socket.io-client": "2.2.0",
    "v-charts": "1.19.0",
    "vue": "2.6.9",
    "vue-airbnb-style-datepicker": "2.6.1",
    "vue-cookie": "1.1.4",
    "vue-fullpage.js": "0.1.2",
    "vue-router": "3.0.2",
    "vuex": "^3.1.2"
  },
  "devDependencies": {
    "@vue/cli-plugin-babel": "3.5.1",
    "@vue/cli-plugin-eslint": "3.5.0",
    "@vue/cli-service": "3.5.1",
    "angular-draggable-droppable": "1.1.1",
    "angular-resizable-element": "1.2.4",
    "babel-eslint": "^10.0.1",
    "calendar-utils": "0.0.59",
    "date-fns": "1.28.5",
    "eslint": "^5.8.0",
    "eslint-config-prettier": "^6.10.0",
    "eslint-loader": "^3.0.3",
    "eslint-plugin-prettier": "^3.1.2",
    "eslint-plugin-vue": "^5.2.2",
    "positioning": "^1.3.1",
    "prettier": "^1.19.1",
    "vue-template-compiler": "2.6.9"
  },
  "eslintConfig": {
    "root": true,
    "env": {
      "node": true,
      "browser": true
    },
    "extends": [
      "plugin:vue/base"
    ],
    "parserOptions": {
      "parser": "babel-eslint"
    },
    "rules": {
      "vue/component-name-in-template-casing": [
        "error",
        "PascalCase"
      ],
      "no-console": "off"
    }
  },
  "postcss": {
    "plugins": {
      "autoprefixer": {}
    }
  },
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "not ie <= 8"
  ]
}
