{
    "name": "PanChangeServer",
    "script": "./app.js",
    "merge_logs": true,
    "out_file": "../../logs/PanChangeServer-dev-pm2-default.log",
    "error_file": "../../logs/PanChangeServer-dev-pm2-error.log",
    "log_date_format": "YYYY-MM-DD HH:mm Z",
    "instances": "1",
    "max_memory_restart": "128M",    
    "wait_ready": true,
    "listen_timeout": 50000,
    "kill_timeout": 5000,
    "env": {
        "NODE_ENV": "development"
    }
}
