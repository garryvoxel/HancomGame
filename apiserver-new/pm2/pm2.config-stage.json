{
    "name": "APIServer",
    "script": "./bin/www",
    "merge_logs": true,
    "out_file": "../../logs/APIServer-stg-pm2-default.log",
    "error_file": "../../logs/APIServer-stg-pm2-error.log",
    "log_date_format": "YYYY-MM-DD HH:mm Z",
    "instances": "4",
    "ignore_watch": ["node_modules"],
    "max_memory_restart": "128M",
    "exec_mode": "cluster",
    "env": {
        "NODE_ENV": "stage"
    }
}
